   "source": [
    "#taking input from user\n",
    "num = int(input(\"Enter a number: \"))\n",
    "i = 1\n",
    "print(f\"Multiplication Table of {num}\")\n",
    "\n",
    "#while loop for multiplication\n",
    "while i <= 12:\n",
    "    print(f\"{num} x {i} = {num * i} \")\n",
    "    i += 1"
   ]
